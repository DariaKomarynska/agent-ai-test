# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4-turbo-preview

# OpenAI Agent SDK Configuration
AGENT_SDK_ENABLED=True
AGENT_THREADS_TTL=3600  # Time to live for agent threads in seconds

# DALL-E Configuration
DALLE_MODEL=dall-e-3

# Application Configuration
APP_HOST=0.0.0.0
APP_PORT=8000
DEBUG=False

# Number of post proposals (default range: 10-20)
MIN_PROPOSALS=10
MAX_PROPOSALS=20

# Search Configuration
SEARCH_ENABLED=True
MAX_SEARCH_RESULTS=5

# Logging
LOG_LEVEL=INFO
